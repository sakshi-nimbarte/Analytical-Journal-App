            <Font size="15.0" />
         </font>
      </PasswordField>
      <Label fx:id="lblstatus" alignment="CENTER" contentDisplay="CENTER" layoutX="94.0" layoutY="197.0" prefHeight="35.0" prefWidth="320.0" textFill="#00ff2f">
